local placeId = game.PlaceId
local subPlaceId = game.PlaceId -- Substitua com a forma de obter o SubPlaceId, se necessário

if placeId == 4954096313 then
    -- Se for o Place principal
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Friez%20Trip%20Z%20Lobby"))()
elseif placeId == 5096191125 or placeId == 5941839954 then
    -- Se for um dos SubPlaces
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Friez%20Trip%20Z%20Game"))()
else
    -- Nenhuma execução de loadstring para outro place
    print("Este place não tem um loadstring vinculado.")
end
