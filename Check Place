-- Tabela de PlaceIDs e SubPlaceIDs com seus respectivos loadstrings
local scripts = {

-- > Friez Trip Z 🧟 Script < --

    -- PlaceID
    [4954096313] = "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Friez%20Trip%20Z%20Lobby",
    
    -- SubPlaceIDs
    [5941839954] = "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Friez%20Trip%20Z%20Game",
    [5096191125] = "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Friez%20Trip%20Z%20Game",
    
-- > Color or Die 🎨 Script < --

    -- PlaceID
    [12931609417] = {
        main = "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Color%20or%20death",
        subPlaces = {
            [13429735204] = "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Color%20or%20death",
            [13622138404] = "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Color%20or%20death",
            [92691286130182] = "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Color%20or%20death",
        }
    }
}

-- Função para carregar o script correto
local function loadScript(placeId)
    local scriptData = scripts[placeId]
    
    if scriptData then
        -- Verifica se existe uma URL para o PlaceID
        if type(scriptData) == "string" then
            loadstring(game:HttpGet(scriptData))()
        elseif type(scriptData) == "table" then
            -- Caso o PlaceID tenha SubPlaceIDs
            local subPlaceId = game.SubPlaceId
            local subScript = scriptData.subPlaces[subPlaceId] or scriptData.main
            loadstring(game:HttpGet(subScript))()
        end
    else
        -- Loadstring padrão caso o placeId não esteja na tabela
        print("Nenhum script correspondente encontrado. Executando script padrão.")
        -- Coloque aqui o loadstring padrão, se necessário
    end
end

-- Verifica o PlaceID atual e carrega o script correspondente
local placeId = game.PlaceId
loadScript(placeId)
