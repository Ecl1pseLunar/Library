-- Tabela que armazena as IDs das places e seus respectivos links
local placeIds = {
    [12931609417] = "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Color%20or%20death",
    [4954096313] = "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/Friez%20trip%20Z"
}

-- Função que tenta executar o loadstring e captura possíveis erros
local function safeExecute(url)
    local success, result = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    
    if success then
        print("Script carregado com sucesso de: " .. url)
    else
        warn("Erro ao carregar o script de: " .. url .. "\nErro: " .. result)
    end
end

-- Função para adicionar ou atualizar links de loadstring para uma nova place ID
local function addOrUpdatePlaceId(placeId, url)
    placeIds[placeId] = url
    print("Place ID " .. placeId .. " foi adicionada/atualizada com o link: " .. url)
end

-- Função principal que verifica a ID da place e executa o loadstring correspondente
local function executeScriptForPlace()
    local currentPlaceId = game.PlaceId
    local scriptUrl = placeIds[currentPlaceId]

    if scriptUrl then
        print("Encontrado script para a place ID " .. currentPlaceId)
        safeExecute(scriptUrl)
    else
        print("Nenhum script vinculado encontrado para a place ID " .. currentPlaceId)
    end
end

-- Executa o script para a place atual
executeScriptForPlace()

-- Exemplo de como adicionar ou atualizar dinamicamente links:
-- addOrUpdatePlaceId(123456790, "https://raw.githubusercontent.com/Ecl1pseLunar/Library/refs/heads/main/NewScript")
